package tests;

public class DataType {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
  //int
		int i = 10;
		i = 20;
		System.out.println(i);
		i= i*2;
		System.out.println(i);
		System.out.println(i*2);
		System.out.println(i);
		System.out.println(i++);
		System.out.println(i);
		System.out.println(++i);
		
	}

}
