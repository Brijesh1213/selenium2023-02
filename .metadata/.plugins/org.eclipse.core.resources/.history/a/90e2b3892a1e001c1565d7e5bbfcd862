package tests;

public class DataType {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
  //int
		int i = 10;
		i = 20;
		System.out.println(i);
		i= i*2;
		System.out.println(i);
		System.out.println(i*2);
		System.out.println(i);
		System.out.println(i++);//post increament next will show
		System.out.println(i);
		System.out.println(++i);//preincreament
	System.out.println("--------"); 
		//char
		char a= 'b';
		System.out.println(a);
		char c= '5';
		System.out.println(c);
		System.out.println("--------"); 
		//double	
		double c1=10.20;
		System.out.println(c1);
	}

}
